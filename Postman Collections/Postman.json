				{
					"name": "Get single booking details",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "a4453a4e-d658-4ce4-aad1-a03425141d43",
								"exec": [
									"pm.test(\"Verifying the status code\", () => {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"var jsonData = pm.response.json();\r",
									"\r",
									"pm.test(\"Verifying the values in JSON response\", () => {\r",
									"    pm.expect(jsonData.firstname).to.eql(\"Canady\");\r",
									"    pm.expect(jsonData.lastname).to.eql(\"Long\");\r",
									"    pm.expect(jsonData.totalprice).to.eql(111);\r",
									"    pm.expect(jsonData.depositpaid).to.eql(true);\r",
									"    pm.expect(jsonData.bookingdates).to.have.property(\"checkin\");\r",
									"    pm.expect(jsonData.bookingdates).to.have.property(\"checkout\");\r",
									"\r",
									"    // Validate the exact date values\r",
									"    pm.expect(jsonData.bookingdates.checkin).to.eql(\"2018-01-01\");\r",
									"    pm.expect(jsonData.bookingdates.checkout).to.eql(\"2019-01-01\");\r",
									"});"
								],
								"type": "text/javascript"
