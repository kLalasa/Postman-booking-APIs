				{
					"name": "Create Booking",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "2016825e-2493-477f-9bcb-950d128b5fa6",
								"exec": [
									"var JsonData=pm.response.json();\r",
									"\r",
									"pm.environment.set(\"booking_id\",JsonData.bookingid);\r",
									"\r",
									"\r",
									"pm.test(\"Test datatype os response\",()=>{\r",
									"    pm.expect(JsonData).to.be.an(\"object\");\r",
									"    pm.expect(JsonData.booking.firstname).to.be.a(\"string\");\r",
									"    pm.expect(JsonData.booking.totalprice).to.be.a(\"number\");\r",
									"    pm.expect(JsonData.booking.depositpaid).to.be.a(\"boolean\")\r",
									"    pm.expect(JsonData.booking.bookingdates.checkin).to.be.a(\"string\");\r",
									"    pm.expect(JsonData.booking.bookingdates.checkout).to.be.a(\"string\");\r",
									"    const checkinDate = new Date(JsonData.booking.bookingdates.checkin);\r",
									"    const checkoutDate = new Date(JsonData.booking.bookingdates.checkout);\r",
									"    pm.expect(checkoutDate).to.be.above(checkinDate)\r",
									"\r",
									"\r",
									"});\r",
									"\r",
									"pm.test(\"Verifying Content-Type Header\",()=>{\r",
									"    pm.response.to.have.header(\"Content-Type\",\"application/json; charset=utf-8\");\r",
									"});\r",
									"pm.test(\"Veryfing the ResponseTime\",()=>{\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
